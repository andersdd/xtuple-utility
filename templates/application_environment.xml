          database    = "{ERP_DATABASE}"
          iss         = "{ERP_ISS}"
          key         = "{ERP_KEY_FILE_PATH}"
          application = "{ERP_APPLICATION}"
          debug       = "{ERP_DEBUG}"/>
</environment>
